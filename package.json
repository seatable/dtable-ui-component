    "clean:lib": "rm -rf lib && mkdir lib",
    "clean:dist": "rm -rf dist && mkdir dist",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "eslint": "eslint --ext .jsx,.js src --max-warnings=0",
